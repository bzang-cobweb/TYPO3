{
    "name": "zang/bza-utils",
    "type": "typo3-cms-extension",
    "description": "Zang utils like, mjml, abstract repository, ...",
    "authors": [],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^10.4",
        "typo3/cms-extbase": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Zang\\BzaUtils\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zang\\BzaUtils\\Tests\\": "Tests"
        }
    },
    "replace": {
        "bza_utils": "self.version",
        "typo3-ter/bza-utils": "self.version"
    }
}
