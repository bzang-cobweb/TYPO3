{
    "name": "bza-main",
    "version": "1.0.0",
    "description": "JavaScript and (S)CSS bundler for bza-main",
    "scripts": {
        "dev": "encore dev --watch",
        "hot": "encore dev-server --https --port 3000",
        "lint": "eslint --fix Resources/Private/Assets/JavaScript/",
        "prod": "encore production"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "homepage": "",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-brands-svg-icons": "^5.15.1",
        "@fortawesome/free-regular-svg-icons": "^5.15.1",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "bootstrap": "^4.5.2",
        "core-js": "^3.8.0",
        "flickity": "^2.2.1",
        "jquery": "^3.5.1",
        "photoswipe": "^4.1.3",
        "popper.js": "^1.16.1",
        "rfs": "^9.0.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@symfony/webpack-encore": "^0.31.0",
        "babel-eslint": "^10.0.3",
        "eslint": "^7.10.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-loader": "^3.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "node-sass": "^4.14.1",
        "sass-loader": "^9.0.1"
    },
    "eslintConfig": {
        "ignorePatterns": [
            "../../../../**/*"
        ],
        "extends": [
            "standard"
        ],
        "parser": "babel-eslint",
        "rules": {
            "indent": [
                "warn",
                4
            ],
            "no-console": "off",
            "no-else-return": "warn",
            "no-new": "off"
        },
        "env": {
            "browser": true
        }
    },
    "browserslist": [
        "defaults, not dead, ie 11"
    ]
}
